package day29_ReturnMethods;

public class returnStatement {
	public static void main(String[] args) {
		System.out.println("Hello World");
		if (true) {
			return;
		}
		System.out.println("Hola"); // Dead code. It will be never printed...

		for (int i = 0; i < 5; i++) {
			if (i == 3) {
				break;
			}
			System.out.println(i);
		}
	}

}
