package TutorialWeek3;

public class ValueOfMethod {
	public static void main(String[] args) {
		String str1="152";
		int num1=Integer.valueOf(str1);    //==> Unboxing/Auto-boxing?
		Integer num2=Integer.valueOf(str1);//==> Unboxing/Auto-boxing?
		System.out.println(num1+2);
		System.out.println(num2+2);
		
		int num3=2019;
		String str3=String.valueOf(num3);
		System.out.println(str3+1);
		
		boolean result1=Boolean.parseBoolean("TRUE"); //==>true
		boolean result2=Boolean.parseBoolean("False");//==>false
		boolean result3=!Boolean.parseBoolean("False");//==>true
		System.out.println(result1);
		System.out.println(result2);
		System.out.println(result3);
		
		boolean b4=4>3;
		String str4=String.valueOf(b4);
		System.out.println(str4);
		
		char ch5='A';
		String str5=String.valueOf(ch5);
		System.out.println(str5);
		
		Integer d1=new Double(12.4).intValue();
		System.out.println(d1);
		Double d2=Double.valueOf("12.4");
		System.out.println(d2);
		Integer iObject=Integer.valueOf("12.4"); //==> returns an Integer Class Value for an object
		int iPrimitive=Integer.parseInt("12"); //==> returns a primitive int value a primitive variable
		System.out.println(iObject);   //==>12
		System.out.println(iPrimitive);//==>12
		
		Integer objectNum=5; // equals to==> " Integer object=new Integer(5); " and it is named Auto-Boxing 
		int primitiveNum=objectNum; // it is named Auto-Unboxing 
		
		
	}

}
