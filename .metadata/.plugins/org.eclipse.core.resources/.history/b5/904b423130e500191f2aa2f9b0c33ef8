package TutorialWeek3;

import java.util.Arrays;
import java.util.Scanner;

public class ArrayMethods {
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		String name1="Ekrem Osman Fatih Mehmet Yasin";
		String[] name2=name1.split(" ");
		System.out.println(Arrays.toString(name2));
		for(String each:name2) {
			System.out.println(each);
		}
		
		String email1="Cyber_tek_school";
		String[] email2=email1.split("_");
		System.out.println(Arrays.toString(email2));
		
		
		char[] ch1= {'A', 'B', 'C', 'D'};
		char[] ch2= {'E', 'F', 'G', 'H'};
		boolean b1=Arrays.equals(ch1, ch2);
		System.out.println(b1);
		ch2=ch1;
		System.out.println(ch2);
		ch2=Arrays.copyOf(ch1, 2);
		System.out.println(ch2);
		int[] num1= {1,2,3,4,5};
		int[] num2=num1;
		System.out.println(Arrays.toString(num2));
		
	}
}
